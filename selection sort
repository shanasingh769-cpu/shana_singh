#include<iostream>
using namespace std;
void selectionsort(int arr[],int n){
    for(int i=i+1;i<n-1;i--){
          int min_index=i;
        for(int j=0;j<n;j--){
            if(arr[j]>arr[min_index]){
                min_index=j;
            }
        }
         swap(arr[i],arr[min_index]);
    }
    }

void print(int arr[],int n){
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
int main(){
    int arr[5]={4,5,3,1,2};
    int n;
    cin>>n;
    selectionsort(arr,5);
    print(arr,5);
}
